<% if @instrument %>
    <%= link_to "Choose existing #{@instrument}", musicians_available_path%> | 
    <%= link_to "Create new #{@instrument}", new_musician_path %>
<%else%>
    <%= link_to 'Practice', band_practice_path(@band), method: 'post' %>
    <%= link_to 'Book a show', venues_path %>
    <%= link_to 'Edit Band', edit_band_path(@band) %>
    <%= link_to 'Disband', band_path(@band), method: 'delete'%>
<% end %>